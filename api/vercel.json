{
  "version": 2,
  "name": "jb-webshop",
  "builds": [
    {
      "src": "api/index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/frontend/build/static/(.*)",
      "dest": "/frontend/build/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/index.html"
    }
  ]
}
