{
  "version": 2,
  "name": "JB-shop-api",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
{
  "version": 2,
  "name": "music-album",
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/index.js"
      }
  ]
}